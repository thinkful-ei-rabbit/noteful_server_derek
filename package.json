    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "winston": "^3.3.3",
    "postgrator-cli": "^3.2.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
